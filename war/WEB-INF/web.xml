<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>JSONstart</servlet-name>
		<servlet-class>com.jsonstart.JSONstartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSONstart</servlet-name>
		<url-pattern>/JSONstartServlet</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>JSONstart1</servlet-name>
		<servlet-class>com.jsonstart.JSONstartServlet1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSONstart1</servlet-name>
		<url-pattern>/JSONstartServlet1</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>JSONstart2</servlet-name>
		<servlet-class>com.jsonstart.JSONstartServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSONstart2</servlet-name>
		<url-pattern>/JSONstartServlet2</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
